== id)\n    }\n  },\n  methods: {\n    editLocation () {\n      const id = parseInt(this.$route.params.id)\n      const locationData = new URLSearchParams()\n      locationData.append('id', id)\n      locationData.append('name', this.location.label)\n      locationData.append('description', this.location.description)\n      this.$store.dispatch('editLocation', locationData)\n    }\n  }\n}\n</script>\n"]}]}