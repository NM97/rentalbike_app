AAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA,EAAA,CAAA,MAAA,CAAA,CAAA;AACA,UAAA,MAAA,GAAA,KAAA,KAAA,IAAA,WAAA,GAAA,QAAA,CAAA;AACA,UAAA,IAAA,GAAA,WAAA,GAAA,QAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA;AACA,aAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAAA;AAIA;AAZA;AAnCA,CAAA","sourcesContent":["<template>\n  <div class=\"max-w-xs bg-white shadow-lg rounded-lg overflow-hidden my-10\">\n    <div class=\"px-4 py-2\">\n      <h1 class=\"text-gray-900 font-bold text-1xl uppercase\">{{name}} {{id}}</h1>\n      <p class=\"text-gray-600 text-sm mt-1\" v-html=\"description\" />\n    </div>\n    <img class=\"h-56 w-full object-cover mt-2\" :src=\"image\" alt=\"bike\" />\n    <h1 class=\"text-gray-900 float-right mr-2\">\n      Cena:\n      <a class=\"text-green-400 font-bold\">{{dates.rprice}} z≈Ç</a>\n    </h1>\n    <br />\n    <div class=\"flex items-center justify-between px-4 py-2 bg-gray-900\">\n      <div class=\"text-gray-200 font-bold text-xl\">\n        <availability-badge :available=\"available\" />\n      </div>\n      <add-to-rental\n        v-if=\"available===1\"\n        :id=\"id\"\n        :name=\"name\"\n        :image=\"image\"\n        :price=\"dates.rprice\"\n        :days=\"dates.days\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { DateTime } from 'luxon'\nimport AvailabilityBadge from '../components/AvailabilityBadge'\nimport AddToRental from '../components/AddToRental'\nexport default {\n  components: {\n    AvailabilityBadge,\n    AddToRental\n  },\n  props: {\n    id: {\n      type: Number,\n      default: null\n    },\n    name: {\n      type: String,\n      required: true\n    },\n    description: {\n      type: String,\n      default: ''\n    },\n    slug: {\n      type: String,\n      default: ''\n    },\n    image: {\n      type: String,\n      default: null\n    },\n    price: {\n      type: Number,\n      default: null\n    },\n    available: {\n      type: Number,\n      default: 1\n    }\n  },\n  computed: {\n    dates () {\n      const start = DateTime.fromISO(this.$store.getters.pickupReservation)\n      const end = DateTime.fromISO(this.$store.getters.dropOffReservation)\n      const daysBetween = end.diff(start, ['days'])\n      const rprice = this.price * (daysBetween / 86400000)\n      const days = (daysBetween / 86400000)\n      console.log(this.id)\n      return {\n        rprice,\n        days\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components"}]}